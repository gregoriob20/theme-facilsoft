<odoo>
    <record id="view_google_font_family_tree" model="ir.ui.view">
        <field name="name">google.font.family.list</field>
        <field name="model">google.font.family</field>
        <field name="arch" type="xml">
            <list string="Google Font Links" editable="bottom">
                <field name="name"/>
                <field name="url"/>
                <field name="is_selected"/>
                <field name="user_id"/>
                <field name="config_id"/>
            </list>
        </field>
    </record>

    <!-- Optional action -->
    <record id="action_google_font_family" model="ir.actions.act_window">
        <field name="name">Google Font Links</field>
        <field name="res_model">google.font.family</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
