#. module: spiffy_theme_backend
#: model_terms:ir.ui.view,arch_db:spiffy_theme_backend.service_worker
msgid ""
"'use strict';\n"
"        var base_cache_ver = 'base-cache-1';\n"
"        var assets = ['/','/pwa/offline'];\n"
"        self.addEventListener('install', function(event) {\n"
"            self.skipWaiting();\n"
"            event.waitUntil(\n"
"                caches.open(base_cache_ver).then(cache =&gt;{\n"
"                    cache.addAll(assets);\n"
"                })\n"
"            )\n"
"        });\n"
"        self.addEventListener('activate', function(event) {\n"
"            event.waitUntil(\n"
"                caches.keys().then(function(keys) {\n"
"                    return Promise.all(keys.map(function(key) {\n"
"                        if (key !== base_cache_ver) {\n"
"                            return caches.delete(key);\n"
"                        }\n"
"                    }));\n"
"                })\n"
"            );\n"
"        });\n"
"        self.addEventListener('fetch', function(event) {\n"
"            event.respondWith(\n"
"                fetch(event.request).then(function (response) {\n"
"                    return response;\n"
"                })\n"
"                .catch(error =&gt; {\n"
"                    return caches.match('/pwa/offline');\n"
"            }));\n"
"        });"
msgstr ""
"'use strict';\n"
"        var base_cache_ver = 'base-cache-1'; // Version du cache\n"
"        var assets = ['/','/pwa/offline']; // Fichiers à mettre en cache\n"
"        self.addEventListener('install', function(event) {\n"
"            self.skipWaiting(); // Passer l'attente\n"
"            event.waitUntil(\n"
"                caches.open(base_cache_ver).then(cache =>{\n"
"                    cache.addAll(assets); // Ajouter les fichiers au cache\n"
"                })\n"
"            )\n"
"        });\n"
"        self.addEventListener('activate', function(event) {\n"
"            event.waitUntil(\n"
"                caches.keys().then(function(keys) {\n"
"                    return Promise.all(keys.map(function(key) {\n"
"                        if (key !== base_cache_ver) {\n"
"                            return caches.delete(key); // Supprimer les anciennes versions du cache\n"
"                        }\n"
"                    }));\n"
"                })\n"
"            );\n"
"        });\n"
"        self.addEventListener('fetch', function(event) {\n"
"            event.respondWith(\n"
"                fetch(event.request).then(function (response) {\n"
"                    return response; // Retourner la réponse du réseau\n"
"                })\n"
"                .catch(error => {\n"
"                    return caches.match('/pwa/offline'); // Page hors ligne en cas d’échec\n"
"            }));\n"
"        });"

